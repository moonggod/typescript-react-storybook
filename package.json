{
  "name": "typescript-react-storybook",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.10.1",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.55",
    "@reduxjs/toolkit": "^1.3.6",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@types/chance": "^1.1.0",
    "@types/js-cookie": "^2.2.6",
    "@types/react-redux": "^7.1.9",
    "@types/react-router-dom": "^5.1.5",
    "axios": "^0.21.1",
    "chance": "^1.1.6",
    "class-transformer": "^0.2.3",
    "class-transformer-validator": "^0.9.0",
    "class-validator": "^0.12.2",
    "enum-values": "^1.2.1",
    "factory.ts": "^0.5.1",
    "i18next": "^19.4.5",
    "js-cookie": "^2.2.1",
    "key-path-mirror": "^1.0.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-i18next": "^11.5.1",
    "react-redux": "^7.2.0",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.1",
    "redux-axios-middleware": "^4.0.1",
    "reflect-metadata": "^0.1.13",
    "routing-controllers": "^0.9.0-alpha.3",
    "uuid": "^8.3.0"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "start:newMock": "cross-env MOCK=api concurrently \"yarn run mock:newAPI\" \"yarn run start\"",
    "build": "react-app-rewired build",
    "build:staging": "env-cmd -f .env.staging npm run build",
    "test": "react-app-rewired test",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public",
    "storybook:newMock": "cross-env MOCK=api concurrently --raw \"yarn run mock:newAPI:storybook\" \"yarn run storybook\"",
    "storybook:env": "cross-env MOCK=env concurrently --raw \"yarn run storybook\"",
    "mock:newAPI": "env-cmd -f .env.development ts-node-dev --project src/mock/tsconfig.json --no-notify src/mock/main.ts",
    "mock:newAPI:storybook": "cross-env TYPE=storybook env-cmd -f .env.development ts-node-dev --project src/mock/tsconfig.json --no-notify src/mock/main.ts"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-decorators": "^7.10.1",
    "@storybook/addon-actions": "^5.3.19",
    "@storybook/addon-info": "^5.3.19",
    "@storybook/addon-links": "^5.3.19",
    "@storybook/addons": "^5.3.19",
    "@storybook/preset-create-react-app": "^3.0.0",
    "@storybook/react": "^5.3.19",
    "@typescript-eslint/eslint-plugin": "^3.2.0",
    "@typescript-eslint/parser": "^3.2.0",
    "babel-loader": "^8.1.0",
    "concurrently": "^5.2.0",
    "cross-env": "^7.0.2",
    "customize-cra": "^1.0.0",
    "env-cmd": "^10.1.0",
    "env-var": "^6.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "http-proxy-middleware": "^1.0.4",
    "prettier": "^2.0.5",
    "react-app-rewired": "^2.1.6",
    "react-docgen-typescript-loader": "^3.7.2",
    "ts-loader": "^7.0.5",
    "ts-node-dev": "^1.0.0-pre.48",
    "typescript": "^3.9.5"
  }
}
